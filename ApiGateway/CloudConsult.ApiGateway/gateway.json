{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/token/generate",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6003
        }
      ],
      "UpstreamPathTemplate": "/api/get-token",
      "UpstreamHttpMethod": [ "Get", "Options" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/user",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6003
        }
      ],
      "UpstreamPathTemplate": "/api/create-user",
      "UpstreamHttpMethod": [ "Post" ]
    },



    {
      "DownstreamPathTemplate": "/api/v1/doctor",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6005
        }
      ],
      "UpstreamPathTemplate": "/api/create-doctor-profile",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/doctor/{DoctorId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6005
        }
      ],
      "UpstreamPathTemplate": "/api/get-doctor-profile/{DoctorId}",
      "UpstreamHttpMethod": [ "Get" ],
      "FileCacheOptions": { "TtlSeconds": 15 }
    },
    {
      "DownstreamPathTemplate": "/api/v1/doctor/{DoctorId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6005
        }
      ],
      "UpstreamPathTemplate": "/api/update-doctor-profile/{DoctorId}",
      "UpstreamHttpMethod": [ "Put" ]
    },



    {
      "DownstreamPathTemplate": "/api/v1/availability",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6009
        }
      ],
      "UpstreamPathTemplate": "/api/add-available-timeslots",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/doctor/{DoctorId}/availability",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6009
        }
      ],
      "UpstreamPathTemplate": "/api/get-available-timeslots/{DoctorId}",
      "UpstreamHttpMethod": [ "Get" ],
      "FileCacheOptions": { "TtlSeconds": 15 }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:6001"
  }
}